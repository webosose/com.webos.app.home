{
	"name": "com.webos.app.home",
	"version": "1.0.0",
	"description": "Home App to list apps and show the status bar",
	"author": "",
	"main": "src/index.js",
	"scripts": {
		"serve": "enact serve",
		"pack": "enact pack",
		"pack-p": "enact pack -p",
		"watch": "enact pack --watch",
		"clean": "enact clean",
		"lint": "enact lint .",
		"license": "enact license",
		"test": "enact test",
		"test-watch": "enact test --watch"
	},
	"license": "UNLICENSED",
	"private": true,
	"repository": "",
	"engines": {
		"npm": ">=6.9.0"
	},
	"enact": {
		"theme": "sandstone",
		"deep": [
			"(window.PalmSystem && window.PalmSystem.launchParams && JSON.parse(window.PalmSystem.launchParams).target) || (window.localStorage.getItem('menuTransparency') && JSON.parse(window.localStorage.getItem('menuTransparency')).menuTransparency==='off') || (window.PalmSystem && window.PalmSystem.highContrast === 'on')"
		]
	},
	"eslintConfig": {
		"extends": "enact"
	},
	"eslintIgnore": [
		"node_modules/*",
		"build/*",
		"dist/*"
	],
	"dependencies": {
		"@enact/core": "^4.5.0",
		"@enact/i18n": "^4.5.0",
		"@enact/sandstone": "^2.5.0",
		"@enact/spotlight": "^4.5.0",
		"@enact/ui": "^4.5.0",
		"@enact/webos": "^4.5.0",
		"ilib": "^14.4.0",
		"prop-types": "^15.6.2",
		"react": "^18.0.0",
		"react-dom": "^18.0.0",
		"react-redux": "^7.2.4",
		"redux": "^4.1.0",
		"redux-thunk": "^2.3.0"
	}
}
