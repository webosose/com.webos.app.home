{
    "name": "com.webos.app.home",
    "id": "home",
    "projectType": "webos-web",
    "sourceLocale": "en-KR",
    "pseudoLocale": ["zxx-XX", "zxx-Hans-XX", "zxx-Cyrl-XX", "zxx-Hebr-XX"],
    "resourceDirs": {
         "json":"resources"
     },
     "resourceFileTypes": {
         "json":"ilib-loctool-webos-json-resource"
     },
    "plugins": [
         "ilib-loctool-webos-javascript",
         "ilib-loctool-webos-json"
     ],
     "excludes": [
         ".*"
     ],
    "settings": {
        "jsonMap": {
            "mappings": {
                "**/appinfo.json": {
                    "template": "[dir]/[resourceDir]/[localeDir]/[filename]"
                }
            }
        },
        "javascript": {
            "disablePseudo": true
        }
    }
}